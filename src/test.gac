    *[ar] >> [ aù   ],
    *[er] >> [ ae   ],
    *[ir] >> [ éa   ],
    *[or] >> [ oe   ],
    *[ur] >> [ eù   ],
    *[al] >> [ au   ],
    *[el] >> [ aeau ],
    *[il] >> [ ieu  ],
    *[ol] >> [ ou   ],
    *[ul] >> [ oû   ]
  },
  *[#(_)=#(C)] >> {
    *[ar] >> [ oî  ],
    *[er] >> [ ea  ],
    *[ir] >> [ éo  ],
    *[or] >> [ oa  ],
    *[ur] >> [ oo  ],
    *[al] >> [ aie ],
    *[el] >> [ oie ],
    *[il] >> [ î   ],
    *[ol] >> [ oue ],
    *[ul] >> [ eou ]
  }
}

*[#(inv)#(V)] -> {
  *[#(inv)a] >> [e],
